{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/favicon.ico",
        "dest": "/favicon.ico"
      },
      {
        "src": "/logo.png",
        "dest": "/logo.png"
      },
      {
        "src": "/manifest.json",
        "dest": "/manifest.json"
      },
      {
        "src": "/attorney-image.jpg",
        "dest": "/attorney-image.jpg"
      },
      {
        "src": "/hero-background.jpg",
        "dest": "/hero-background.jpg"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }